<?xml version="1.0" encoding="UTF-8"?>
<workflow xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="workflowDefinition.xsd">
    <name>A-move-files-wf</name>
    <externalWorkflowCode>A-move-files-wf</externalWorkflowCode>
    <finishOrchestrationASAPOnError>false</finishOrchestrationASAPOnError>
    <description>Testing utility workflow to move source files</description>
    
    <stage enabled="true" id="move-files" name="move-files">
        <execDefinition>
            <callExternalProcess>
                <commandLine>/tmp/movefiles.sh</commandLine>
                <workingDirectory>/tmp</workingDirectory>
                <exitCodeProperty>exitCode</exitCodeProperty>
            </callExternalProcess>
        </execDefinition>
    </stage>
</workflow>
