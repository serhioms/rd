<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- $Id: ctr-eod-acbs-wf.xml 4168 2016-06-23 19:16:47Z gmusial $
	 Maestro version: 1.19.1 -->
	 
<workflow xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="workflowDefinition.xsd">
	<name>ctr-eod-acbs-wf</name> 
  	<externalWorkflowCode>ctr-eod-acbs-wf</externalWorkflowCode> 
  	<finishOrchestrationASAPOnError>false</finishOrchestrationASAPOnError> 
	<description>ctr-eod-acbs-wf ($LastChangedRevision: 4168 $)</description>
	
	<properties>
		<inputProperties>
			<property name="WF-EOD-EOM-INDICATOR">D</property>
			<property name="RUN-INSTANCE">1</property>
		</inputProperties>
		
		<generalProperties>
			<property name="ROOT-WF-ID">ctr-eod-acbs-wf</property>
			<property name="WORKFLOW_RUN_ID">0</property>
			<!-- Source Directories -->
			<property name="ACBS-FTP-SOURCE-DIR">acbs/inbox</property>
			<property name="ACBS-HOLDING-DIR">${CTR_DATA_ROOT_PATH}/manual/acbs</property>
			<property name="ACBS-HOLDING-MATCH-PATTERN">.+_${WF-EOD-EOM-INDICATOR}_${INITIALIZE-RUN.BUSINESS-DATE-FORMATTED}_.+</property>
			<property name="SQLLDR-PROCESSING-SOURCE-DIR">data_in/infiles</property>
			<property name="SQLLDR-CONTROL-DIRECTORY">sqlldr</property>
			<property name="SQLLDR-DISCARD-DIR">data_in/discard</property>
			<property name="SQLLDR-REJECT-DIRECTORY">data_in/reject</property>
			<property name="SQLLDR-LOG-DIRECTORY">data_in/ldrlog</property>
			
			<property name="REPLICATE-SRC-IF-MONTH-END">F</property>
			<property name="DATA-CATEGORY-TYPE">LOANS</property>
			
			<!-- Source file naming pattern -->
			<property name="SRC-FILE-PRODUCTTYPE-PATTERN">[^_]*[_]?</property>
			<property name="SRC-FILE-FILETYPE-PATTERN">[PT]?[_]?</property>
			<property name="SRC-FILE-FREQUENCY-PATTERN">${WF-EOD-EOM-INDICATOR}_</property>
			<property name="SRC-FILE-VERSION-PATTERN">(${INITIALIZE-RUN.BUSINESS-DATE-FORMATTED})?_[0-9]{2,2}</property>
			<property name="SRC-FILE-SUFFIX-PATTERN-1">${SRC-FILE-PRODUCTTYPE-PATTERN}${SRC-FILE-FILETYPE-PATTERN}${SRC-FILE-FREQUENCY-PATTERN}${SRC-FILE-VERSION-PATTERN}</property>
			
		</generalProperties>
	</properties>	

	<sequential name="CTR-LOANS" id="CTR-LOANS">
	
		<externalWorkflowReference name="INITIALIZE-RUN" id="INITIALIZE-RUN">
			<externalWorkflowCode>INITIALIZE-RUN</externalWorkflowCode>
			<inputProperties>
				<property name="EOD-EOM-INDICATOR">${WF-EOD-EOM-INDICATOR}</property>
				<property name="SOURCE-SYSTEM">ACBS</property>
				<property name="ROOT-WF-ID">${ROOT-WF-ID}</property>
				<property name="TARGET-SYSTEM"></property>
				<property name="RUN-TYPE" />
				<property name="RUN-INSTANCE">${RUN-INSTANCE}</property>
				<property name="WORKFLOW-RUN-ID">${WORKFLOW_RUN_ID}</property>
			</inputProperties>
			<outputPropertyMapping>
				<mapping source="BUSINESS-DATE" target="BUSINESS-DATE"/>
				<mapping source="BUSINESS-DATE-FORMATTED" target="BUSINESS-DATE-FORMATTED"/>
				<mapping source="RUN-ID" target="P_RUN_ID_O"/>
				<mapping source="IS-MONTH-END" target="IS-MONTH-END"/>
			</outputPropertyMapping>
		</externalWorkflowReference>
		<externalWorkflowReference name="PREPARE-SOURCE-FILES" id="PREPARE-SOURCE-FILES">
			<externalWorkflowCode>PREPARE-SOURCE-FILES</externalWorkflowCode>
			<inputProperties>
				<property name="FTP-SOURCE-DIR">${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</property>
				<property name="HOLDING-SOURCE-DIR">${ACBS-HOLDING-DIR}</property>
				<property name="HOLDING-MATCH-PATTERN">${ACBS-HOLDING-MATCH-PATTERN}</property>
				<property name="IS-MONTH-END">${INITIALIZE-RUN.IS-MONTH-END}</property>
				<property name="EOD-EOM-INDICATOR">${WF-EOD-EOM-INDICATOR}</property>
			</inputProperties>
		</externalWorkflowReference>
		<stage name="INIT-COMPLETE-CHECK" id="INIT-COMPLETE-CHECK">
			<description>Verify if the common workflow steps completed</description>
			<stageFailure>
				<failureCondition><![CDATA[${INITIALIZE-RUN.P_RUN_ID_O} <= 0 || "${CTR_APP_ROOT_PATH}" == '' || "${CTR_ROOT_PATH}" == ''|| "${CTR_FTP_ROOT_PATH}" == ''|| "${CTR_DATA_ROOT_PATH}" == ''|| "${CTR_CONFIG_ROOT_PATH}" == '']]></failureCondition>
				<failureMessage>Common workflow initialization failed</failureMessage>
			</stageFailure>
			<execDefinition>
				<noOpExecDefinition>
					<input>CHECK-IF-INIT-COMPLETE</input>
					<numberOfTasks>1</numberOfTasks>
					<numberOfInterimStatusResponses>1</numberOfInterimStatusResponses>
				</noOpExecDefinition>
			</execDefinition>
		</stage>
		
		<!-- LOANS-WORKFLOWS -->
		
		<parallel id="LOANS-WORKFLOWS" name="LOANS-WORKFLOWS">
			
			<sequential id="ACBS" name="ACBS">	
				<stage name="ACBS-FILEWATCH-WF" id="ACBS-FILEWATCH-WF">
					<autoRetryConfiguration>
						<delayBetweenAttemptsInMs>60000</delayBetweenAttemptsInMs>
                        <maximumNumberOfRetryAttempts>1080</maximumNumberOfRetryAttempts>
                        <maximumRetryDurationInMs>64800000</maximumRetryDurationInMs>
					</autoRetryConfiguration>
					<execDefinition>
						<pollingFileWatcher>
							<watchedFiles>
								<resource>
									<uri>${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</uri>
									<searchPattern>ACBS_PA_${SRC-FILE-SUFFIX-PATTERN-1}.csv</searchPattern>
									<matchedUriOutputProperty>PA-FILE</matchedUriOutputProperty>
								</resource>
								<resource>
									<uri>${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</uri>
									<searchPattern>ACBS_PA_${SRC-FILE-SUFFIX-PATTERN-1}.mrk</searchPattern>
									<matchedUriOutputProperty>PA-FILE-MRK</matchedUriOutputProperty>
								</resource>
								<resource>
									<uri>${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</uri>
									<searchPattern>ACBS_LO_${SRC-FILE-SUFFIX-PATTERN-1}.csv</searchPattern>
									<matchedUriOutputProperty>LO-FILE</matchedUriOutputProperty>
								</resource>
								<resource>
									<uri>${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</uri>
									<searchPattern>ACBS_LO_${SRC-FILE-SUFFIX-PATTERN-1}.mrk</searchPattern>
									<matchedUriOutputProperty>LO-FILE-MRK</matchedUriOutputProperty>
								</resource>
								<resource>
									<uri>${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</uri>
									<searchPattern>ACBS_XR_${SRC-FILE-SUFFIX-PATTERN-1}.csv</searchPattern>
									<matchedUriOutputProperty>XR-FILE</matchedUriOutputProperty>
								</resource>
								<resource>
									<uri>${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</uri>
									<searchPattern>ACBS_XR_${SRC-FILE-SUFFIX-PATTERN-1}.mrk</searchPattern>
									<matchedUriOutputProperty>XR-FILE-MRK</matchedUriOutputProperty>
								</resource>
								<resource>
									<uri>${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</uri>
									<searchPattern>ACBS_FA_${SRC-FILE-SUFFIX-PATTERN-1}.csv</searchPattern>
									<matchedUriOutputProperty>FA-FILE</matchedUriOutputProperty>
								</resource>
								<resource>
									<uri>${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</uri>
									<searchPattern>ACBS_FA_${SRC-FILE-SUFFIX-PATTERN-1}.mrk</searchPattern>
									<matchedUriOutputProperty>FA-FILE-MRK</matchedUriOutputProperty>
								</resource>
							</watchedFiles>
							<matchedCountProperty>matchedCount</matchedCountProperty>
						</pollingFileWatcher>
					</execDefinition>
				</stage>
				<parallel id="LOANS-CHECKSUM" name="CHECKSUM">
					<stage name="CHECKSUM-FILE-1" id="LOANS-CHK-1">
						<description>Validate checksum for file 1</description>
						<execDefinition>
							<checksumValidation>
								<sourceResource>
									<uri>${ACBS-FILEWATCH-WF.PA-FILE}</uri>
								</sourceResource>
								<markerResource>
									<uri>${ACBS-FILEWATCH-WF.PA-FILE-MRK}</uri>
								</markerResource>
								<checksumAlgorithm>SHA256</checksumAlgorithm>
							</checksumValidation>
						</execDefinition>
					</stage>
					<stage name="CHECKSUM-FILE-2" id="LOANS-CHK-2">
						<description>Validate checksum for file 2</description>
						<execDefinition>
							<checksumValidation>
								<sourceResource>
									<uri>${ACBS-FILEWATCH-WF.LO-FILE}</uri>
								</sourceResource>
								<markerResource>
									<uri>${ACBS-FILEWATCH-WF.LO-FILE-MRK}</uri>
								</markerResource>
								<checksumAlgorithm>SHA256</checksumAlgorithm>
							</checksumValidation>
						</execDefinition>
					</stage>
					<stage name="CHECKSUM-FILE-3" id="LOANS-CHK-3">
						<description>Validate checksum for file 3</description>
						<execDefinition>
							<checksumValidation>
								<sourceResource>
									<uri>${ACBS-FILEWATCH-WF.XR-FILE}</uri>
								</sourceResource>
								<markerResource>
									<uri>${ACBS-FILEWATCH-WF.XR-FILE-MRK}</uri>
								</markerResource>
								<checksumAlgorithm>SHA256</checksumAlgorithm>
							</checksumValidation>
						</execDefinition>
					</stage>
					<stage name="CHECKSUM-FILE-4" id="LOANS-CHK-4">
						<description>Validate checksum for file 4</description>
						<execDefinition>
							<checksumValidation>
								<sourceResource>
									<uri>${ACBS-FILEWATCH-WF.FA-FILE}</uri>
								</sourceResource>
								<markerResource>
									<uri>${ACBS-FILEWATCH-WF.FA-FILE-MRK}</uri>
								</markerResource>
								<checksumAlgorithm>SHA256</checksumAlgorithm>
							</checksumValidation>
						</execDefinition>
					</stage>
				</parallel>
				<externalWorkflowReference name="ACBS-PREPARE-LOAD-WF" id="ACBS-PREPARE-LOAD-WF">
					<skipCondition>${ACBS-FILEWATCH-WF.matchedCount} == 0</skipCondition>
					<externalWorkflowCode>PREPARE-LOANS-LOAD-WF</externalWorkflowCode>
					<inputProperties>
						<property name="SOURCE-SYSTEM">ACBS</property>
						<property name="SOURCE-SYSTEM-LC">acbs</property>
						<property name="BUSINESS-DATE">${INITIALIZE-RUN.BUSINESS-DATE}</property>
						<property name="RUN-ID">${INITIALIZE-RUN.P_RUN_ID_O}</property>
						<property name="FTP-SOURCE-DIR">${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</property>
						<property name="PROCESSING-DIR">${CTR_DATA_INBOUND_PATH}</property>
						<property name="HOLDING-SOURCE-DIR">${ACBS-HOLDING-DIR}</property>
						<property name="REPLICATE-IF-MONTH-END">${REPLICATE-SRC-IF-MONTH-END}</property>
						<property name="IS-MONTH-END">${INITIALIZE-RUN.IS-MONTH-END}</property>
						<property name="EOD-EOM-INDICATOR">${WF-EOD-EOM-INDICATOR}</property>
					</inputProperties>
				</externalWorkflowReference>
				<parallel id="ACBS-LOAD" name="ACBS-LOAD-SOURCE-FILES">
					<skipCondition>${ACBS-FILEWATCH-WF.matchedCount} == 0</skipCondition>
					<externalWorkflowReference name="ACBS-LOAD-PARTY" id="ACBS-LOAD-PARTY">
						<externalWorkflowCode>LOAD-FILE-WF</externalWorkflowCode>
						<inputProperties>
							<property name="RUN-ID">${INITIALIZE-RUN.P_RUN_ID_O}</property>
							<property name="BUSINESS-DATE">${INITIALIZE-RUN.BUSINESS-DATE}</property>
							<property name="SOURCE-FILE">${ACBS-FILEWATCH-WF.PA-FILE}</property>
							<property name="PROCESSING-SOURCE-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-PROCESSING-SOURCE-DIR}</property>
							<property name="CONTROL-PATH">${CTR_CONFIG_ROOT_PATH}/${SQLLDR-CONTROL-DIRECTORY}</property>
							<property name="DISCARD-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-DISCARD-DIR}</property>
							<property name="REJECT-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-REJECT-DIRECTORY}</property>
							<property name="LOG-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-LOG-DIRECTORY}</property>
							<property name="DATA-CATEGORY">${DATA-CATEGORY-TYPE}</property>
							<property name="DELIMITER">,</property>
							
						</inputProperties>
					</externalWorkflowReference>
					<externalWorkflowReference name="ACBS-LOAD-LOAN" id="ACBS-LOAD-LOAN">
						<externalWorkflowCode>LOAD-FILE-WF</externalWorkflowCode>
						<inputProperties>
							<property name="RUN-ID">${INITIALIZE-RUN.P_RUN_ID_O}</property>
							<property name="BUSINESS-DATE">${INITIALIZE-RUN.BUSINESS-DATE}</property>
							<property name="SOURCE-FILE">${ACBS-FILEWATCH-WF.LO-FILE}</property>
							<property name="PROCESSING-SOURCE-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-PROCESSING-SOURCE-DIR}</property>
							<property name="CONTROL-PATH">${CTR_CONFIG_ROOT_PATH}/${SQLLDR-CONTROL-DIRECTORY}</property>
							<property name="DISCARD-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-DISCARD-DIR}</property>
							<property name="REJECT-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-REJECT-DIRECTORY}</property>
							<property name="LOG-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-LOG-DIRECTORY}</property>
							<property name="DATA-CATEGORY">${DATA-CATEGORY-TYPE}</property>
							<property name="DELIMITER">,</property>
							
						</inputProperties>
					</externalWorkflowReference>
					<externalWorkflowReference name="ACBS-LOAD-CROSS-REF" id="ACBS-LOAD-CROSS-REF">
						<externalWorkflowCode>LOAD-FILE-WF</externalWorkflowCode>
						<inputProperties>
							<property name="RUN-ID">${INITIALIZE-RUN.P_RUN_ID_O}</property>
							<property name="BUSINESS-DATE">${INITIALIZE-RUN.BUSINESS-DATE}</property>
							<property name="SOURCE-FILE">${ACBS-FILEWATCH-WF.XR-FILE}</property>
							<property name="PROCESSING-SOURCE-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-PROCESSING-SOURCE-DIR}</property>
							<property name="CONTROL-PATH">${CTR_CONFIG_ROOT_PATH}/${SQLLDR-CONTROL-DIRECTORY}</property>
							<property name="DISCARD-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-DISCARD-DIR}</property>
							<property name="REJECT-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-REJECT-DIRECTORY}</property>
							<property name="LOG-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-LOG-DIRECTORY}</property>
							<property name="DATA-CATEGORY">${DATA-CATEGORY-TYPE}</property>
							<property name="DELIMITER">,</property>
							
						</inputProperties>
					</externalWorkflowReference>
					<externalWorkflowReference name="ACBS-LOAD-FACILITY" id="ACBS-LOAD-FACILITY">
						<externalWorkflowCode>LOAD-FILE-WF</externalWorkflowCode>
						<inputProperties>
							<property name="RUN-ID">${INITIALIZE-RUN.P_RUN_ID_O}</property>
							<property name="BUSINESS-DATE">${INITIALIZE-RUN.BUSINESS-DATE}</property>
							<property name="SOURCE-FILE">${ACBS-FILEWATCH-WF.FA-FILE}</property>
							<property name="PROCESSING-SOURCE-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-PROCESSING-SOURCE-DIR}</property>
							<property name="CONTROL-PATH">${CTR_CONFIG_ROOT_PATH}/${SQLLDR-CONTROL-DIRECTORY}</property>
							<property name="DISCARD-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-DISCARD-DIR}</property>
							<property name="REJECT-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-REJECT-DIRECTORY}</property>
							<property name="LOG-PATH">${CTR_DATA_ROOT_PATH}/${SQLLDR-LOG-DIRECTORY}</property>
							<property name="DATA-CATEGORY">${DATA-CATEGORY-TYPE}</property>
							<property name="DELIMITER">,</property>
							
						</inputProperties>
					</externalWorkflowReference>
				</parallel>
				<externalWorkflowReference name="ACBS-ETL-WF" id="ACBS-ETL-WF">
					<skipCondition>${ACBS-FILEWATCH-WF.matchedCount} == 0</skipCondition>
					<externalWorkflowCode>LOANS-ETL-WF</externalWorkflowCode>
					<inputProperties>
						<property name="SOURCE-SYSTEM">ACBS</property>
						<property name="SOURCE-SYSTEM-LC">acbs</property>
						<property name="BUSINESS-DATE">${INITIALIZE-RUN.BUSINESS-DATE}</property>
						<property name="RUN-ID">${INITIALIZE-RUN.P_RUN_ID_O}</property>
					</inputProperties>
				</externalWorkflowReference>
				<externalWorkflowReference name="ACBS-DELIVER-WF" id="ACBS-DELIVER-WF">
					<skipCondition>${ACBS-FILEWATCH-WF.matchedCount} == 0</skipCondition>
					<externalWorkflowCode>LOANS-EXTRACT-TO-ISL</externalWorkflowCode>
					<inputProperties>
						<property name="SOURCE-SYSTEM">ACBS</property>
						<property name="SOURCE-SYSTEM-LC">acbs</property>
						<property name="RUN-ID">${INITIALIZE-RUN.P_RUN_ID_O}</property>
						<property name="BUSINESS-DATE">${INITIALIZE-RUN.BUSINESS-DATE-FORMATTED}</property>
						<property name="EOD-EOM-IND">${WF-EOD-EOM-INDICATOR}</property>
					</inputProperties>
				</externalWorkflowReference>
			</sequential>
		</parallel>
		
		<!-- FINALIZE PHASE -->
		
		<externalWorkflowReference name="FINALIZE-RUN" id="COMPLETE-RUN">
			<description>Finalize the run and mark complete</description>
			<externalWorkflowCode>FINALIZE-RUN</externalWorkflowCode>
			<inputProperties>
				<property name="SOURCE-SYSTEM">ACBS</property>
				<property name="RUN-ID">${INITIALIZE-RUN.P_RUN_ID_O}</property>
				<property name="BUSINESS-DATE">${INITIALIZE-RUN.BUSINESS-DATE}</property>
				<property name="SRC-FILE-SUFFIX-PATTERN">${SRC-FILE-SUFFIX-PATTERN-1}</property>
				<property name="FTP-SOURCE-DIR">${CTR_FTP_ROOT_PATH}/${ACBS-FTP-SOURCE-DIR}</property>
			</inputProperties>
		</externalWorkflowReference>
	</sequential>
</workflow>