      <xs:annotation>
         <xs:documentation>List of properties to be passed to the Java Extension, represented in CDATA blocks in the Workflow XML.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="propertyString">
            <xs:annotation>
               <xs:documentation>CDATA block for this property</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="2048"/>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
