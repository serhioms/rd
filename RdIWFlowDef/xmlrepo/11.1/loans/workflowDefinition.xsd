         <xs:element name="sourceResource" type="resource">
            <xs:annotation>
               <xs:documentation>Resource for the source file.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="markerResource" type="resource">
            <xs:annotation>
               <xs:documentation>Resource containing the marker file.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="checksumAlgorithm" type="checksumAlgorithmTypeEnum">
            <xs:annotation>
               <xs:documentation>Checksum algorithm to use.  One of 'MD5', 'SHA1', or 'SHA256'.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="checksumGeneration">
      <xs:annotation>
         <xs:documentation>Execution Definition which generates checksums for one or multiple files.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="sourceResource" type="resource">
            <xs:annotation>
               <xs:documentation>Resource for the file for with the checksum will be generated.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="markerResource" type="resource">
            <xs:annotation>
               <xs:documentation>Resource to which the marker file will be written.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="checksumAlgorithm" type="checksumAlgorithmTypeEnum">
            <xs:annotation>
               <xs:documentation>Checksum algorithm to use.  One of 'MD5', 'SHA1', or 'SHA256'.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="callExternalProcess">
      <xs:annotation>
         <xs:documentation>Execution Definition which invokes a system process.  This can be another program, script, or executable. </xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="commandLine">
            <xs:annotation>
               <xs:documentation>The command line string which will be passed to the system for execution.  On Unix systems, this is simply what you would type into the terminal.  On Windows systems, this may need to be prefixed with "cmd".</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="1024"/>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="workingDirectory">
            <xs:annotation>
               <xs:documentation>Filesystem directory from which the command line will be executed.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="1024"/>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="exitCodeProperty">
            <xs:annotation>
               <xs:documentation>The name of the Maestro Workflow property which will store the exit flag from running this system process.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="255"/>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="sqlLoader">
      <xs:annotation>
         <xs:documentation>Execution Definition which loads bulk data using Oracle SQL*Loader.</xs:documentation>
      </xs:annotation>
