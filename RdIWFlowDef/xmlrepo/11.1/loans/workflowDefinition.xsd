               <xs:restriction base="xs:string">
                  <xs:maxLength value="1024"/>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="dataFileUri" minOccurs="0">
            <xs:annotation>
               <xs:documentation>URI for the data file which will be loaded by SQL*Loader.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="1024"/>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="discardFileUri">
            <xs:annotation>
               <xs:documentation>URI for the SQL*Loader discard file.  SQL*Loader will write discarded records to this file.  For more information on discard files, see the SQL*Loader documentation.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="1024"/>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="rejectFileUri">
            <xs:annotation>
               <xs:documentation>URI for the SQL*Loader reject file.  SQL*Loader will write rejected records to this file.  See the SQL*Loader documentation for more information on reject files.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="1024"/>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="logFileUri">
            <xs:annotation>
               <xs:documentation>URI for the SQL*Loader log file.  The SQL*Loader log will be written to this file.</xs:documentation>
