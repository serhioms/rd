                  <xs:minLength value="0"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:choice>
            <xs:annotation>
               <xs:documentation>Input properties for this Java extension specified as simple key/value pairs.</xs:documentation>
            </xs:annotation>
            <xs:element ref="simpleProperties"/>
            <xs:element ref="cdataProperties"/>
         </xs:choice>
         <xs:element name="javaExtensionOutputPropertyNames" minOccurs="0">
            <xs:annotation>
               <xs:documentation>List of output properties from the implementation of IMaestroJavaExtensionExecutor for this extension.  These properties will be available as variables in the Maestro Workflow context.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="propertyName"
                              minOccurs="0"
                              maxOccurs="99">
                     <xs:annotation>
