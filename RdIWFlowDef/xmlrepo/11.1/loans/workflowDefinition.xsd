         <xs:element ref="transfer"
                     minOccurs="1"
                     maxOccurs="99">
            <xs:annotation>
               <xs:documentation>A file transfer.  Requires a source and target resource.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="transferCountProperty">
            <xs:annotation>
               <xs:documentation>Name of the Maestro Workflow property which will hold the number of files transfered.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="255"/>
                  <xs:minLength value="1"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="transfer">
      <xs:annotation>
         <xs:documentation>Execution Definition for a File Transfer.  Transfers one or more files between local or remote locations.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="sourceResource" type="resource">
            <xs:annotation>
               <xs:documentation>Source file resource.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="targetResource" type="resource">
            <xs:annotation>
               <xs:documentation>Target file resource.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="deleteResource">
      <xs:annotation>
         <xs:documentation>Execution Definition that deletes a filesystem resource.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element ref="resource"
                     minOccurs="1"
                     maxOccurs="99">
            <xs:annotation>
               <xs:documentation>Resource to delete.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="deleteCountProperty">
            <xs:annotation>
               <xs:documentation>Name of the Maestro Workflow property that will store the number of files/resources deleted.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="255"/>
