                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:maxLength value="255"/>
                           <xs:minLength value="1"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="outputPropertyMapping" type="propertyMapping" minOccurs="0"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="simpleProperties">
      <xs:annotation>
         <xs:documentation>List of simple key/value pairs to pass to the Java extension.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="properties" minOccurs="0">
            <xs:annotation>
               <xs:documentation>List of simple key/value pairs to pass to the Java extension class.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element ref="simpleProperty"
                              minOccurs="0"
                              maxOccurs="99">
                     <xs:annotation>
                        <xs:documentation>Simple key/value property to pass to the Java extension class.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="simpleProperty">
      <xs:annotation>
         <xs:documentation>Simple key/value property to pass to the Java extension class.</xs:documentation>
      </xs:annotation>
      <xs:simpleContent>
         <xs:extension base="simpleProperty.restrictedLength">
            <xs:attribute name="name" use="required">
               <xs:annotation>
                  <xs:documentation>Name of this property.</xs:documentation>
               </xs:annotation>
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:maxLength value="255"/>
                     <xs:minLength value="1"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
         </xs:extension>
      </xs:simpleContent>
   </xs:complexType>
   <xs:simpleType name="simpleProperty.restrictedLength">
      <xs:restriction base="xs:string">
         <xs:maxLength value="1024"/>
         <xs:minLength value="0"/>
      </xs:restriction>
   </xs:simpleType>
