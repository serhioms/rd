   <xs:complexType name="javaExtension">
      <xs:annotation>
         <xs:documentation>Execution Definition which executes a custom Java extension.  The Java extension must be available on the Batch Stage Processor's classpath, and implement IMaestroJavaExtensionExecutor.</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="javaExtensionExecutorClassName">
            <xs:annotation>
