<workflow xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="workflowDefinition.xsd">
	<name>set-ctrdb-env-wf</name> 
  	<externalWorkflowCode>set-ctrdb-env-wf</externalWorkflowCode> 
  	<finishOrchestrationASAPOnError>false</finishOrchestrationASAPOnError> 
	<description>set-ctrdb-env-wf($LastChangedRevision: 3690 $)</description>
	
	<sequential name="ctrdb-env" id="ctrdb-env">		
		<stage name="ctrdb-env-stage" id="ctrdb-env-stage">
			<description>ctrdb env set</description>
			<execDefinition>
				<dbProcedure name="CTRMSO.SET_CTRDB_CLASS_SP">
					<in>
						<param name="P_CTRDB_CLASS_I"  type="STRING">${MAESTRO_RUN_ENV}</param>
					</in>
				</dbProcedure>
			</execDefinition>
		</stage>
	</sequential>
</workflow>