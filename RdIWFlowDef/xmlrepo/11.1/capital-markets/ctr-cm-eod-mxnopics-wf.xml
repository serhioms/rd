							<checksumValidation>
								<sourceResource>
									<uri>${FILEWATCH-WF.PA-Entity-FILE}</uri>
								</sourceResource>
								<markerResource>
									<uri>${FILEWATCH-WF.PA-Entity-MRK}</uri>
								</markerResource>
								<checksumAlgorithm>MD5</checksumAlgorithm>
							</checksumValidation>
						</execDefinition>
					</stage>
					<stage name="CHECKSUM-CM-MEASURES" id="CHK-2">
						<description>Validate checksum for file CM-Measures</description>
