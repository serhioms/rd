				<stage name="LAND-TO-TARGET-CASH-HOLDING" id="IN-CASH-HOLDING-PREL2LT">
					<description>NON-MMI CASH HOLDING loading from landing to target</description>
  				    <skipCondition> not ('${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list1_o}'.contains('[cash_holding_tp.pub_move_land_2_target]') or '${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list2_o}'.contains('[cash_holding_tp.pub_move_land_2_target]'))</skipCondition>
					<execDefinition>
						<dbProcedure name="CTRMSO.ETL_INVOKER_SP">
							<in>
								<param name="P_TYPE_NAME_I" type="STRING">cash_holding_tp</param>
