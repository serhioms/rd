			<stage name="LAND-TO-TARGET-COLLATERAL" id="CO-COLLATERAL-L2T">
				<description>Sentry collateral loading from landing to target</description>
  				    <skipCondition> not ('${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list1_o}'.contains('[collateral_tp.pub_move_land_2_target]') or '${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list2_o}'.contains('[collateral_tp.pub_move_land_2_target]'))</skipCondition>
