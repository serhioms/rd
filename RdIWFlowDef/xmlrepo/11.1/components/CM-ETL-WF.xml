				<!-- Load MMI cash holding -->
				<stage name="LAND-TO-TARGET-MMI-CASH-HOLDING" id="IN-MMI-CASH-HOLDING-PREL2LT">
					<description>MMI CASH HOLDING loading from landing to target</description>
  				    <skipCondition> not ('${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list1_o}'.contains('[mmi_cash_holding_tp.pub_move_land_2_target]') or '${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list2_o}'.contains('[mmi_cash_holding_tp.pub_move_land_2_target]'))</skipCondition>
