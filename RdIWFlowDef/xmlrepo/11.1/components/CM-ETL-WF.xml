  				    <skipCondition> not ('${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list1_o}'.contains('[repo_tp.pub_move_land_2_target]') or '${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list2_o}'.contains('[repo_tp.pub_move_land_2_target]'))</skipCondition>
