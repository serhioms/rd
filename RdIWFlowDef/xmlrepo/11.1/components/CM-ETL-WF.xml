				</stage>
				<!-- Load Basket -->
				<stage name="LAND-TO-TARGET-BASKET" id="IN-BASKET-L2T">
					<description>BASKET loading from landing to target</description>
  				    <skipCondition> not ('${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list1_o}'.contains('[basket_tp.pub_move_land_2_target]') or '${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list2_o}'.contains('[basket_tp.pub_move_land_2_target]'))</skipCondition>
					<execDefinition>
						<dbProcedure name="CTRMSO.ETL_INVOKER_SP">
							<in>
								<param name="P_TYPE_NAME_I" type="STRING">basket_tp</param>
								<param name="P_METHOD_NAME_I" type="STRING">pub_move_land_2_target</param>
								<param name="P_RUN_ID_I" type="NUMBER">${RUN-ID}</param>
								<param name="P_STAGE_ID_I" type="STRING">TARGET</param>
