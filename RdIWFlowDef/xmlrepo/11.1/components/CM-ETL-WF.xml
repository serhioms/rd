								<param name="P_BUSINESS_DATE_I" type="STRING">${BUSINESS-DATE}</param>
								<param name="P_SOURCE_SYSTEM_CD_I" type="STRING">${SOURCE-SYSTEM}</param>
							</in>
							<out>
								<param name="P_RET_VALUE_O" type="NUMBER"></param>
								<param name="P_RET_MESSAGE_O" type="STRING"></param>
							</out>
						</dbProcedure>
					</execDefinition>
				</stage>
				
				<stage name="LAND-TO-TARGET-SBLEXPOSURE" id="CM-SBLEXPOSURE-L2T">
					<description>SBL exposure loading from landing to target</description>
  				    <skipCondition> not ('${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list1_o}'.contains('[sbl_exposure_tp.pub_move_land_2_target]') or '${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list2_o}'.contains('[sbl_exposure_tp.pub_move_land_2_target]'))</skipCondition>
					<execDefinition>
						<dbProcedure name="CTRMSO.ETL_INVOKER_SP">
							<in>
								<param name="P_TYPE_NAME_I" type="STRING">sbl_exposure_tp</param>
								<param name="P_METHOD_NAME_I" type="STRING">pub_move_land_2_target</param>
								<param name="P_RUN_ID_I" type="NUMBER">${RUN-ID}</param>
								<param name="P_STAGE_ID_I" type="STRING">TARGET</param>
								<param name="P_TASK_ID_I" type="STRING">CM-SBLEXPOSURE-L2T</param>
								<param name="P_LOG_ID_I" type="STRING">1</param>
								<param name="P_BUSINESS_DATE_I" type="STRING">${BUSINESS-DATE}</param>
								<param name="P_SOURCE_SYSTEM_CD_I" type="STRING">${SOURCE-SYSTEM}</param>
							</in>
							<out>
								<param name="P_RET_VALUE_O" type="NUMBER"></param>
								<param name="P_RET_MESSAGE_O" type="STRING"></param>
							</out>
						</dbProcedure>
					</execDefinition>
				</stage>
				
				
				
				<!-- Load holding -->
				<stage name="LAND-TO-TARGET-HOLDING" id="IN-HOLDING-L2T">
					<description>HOLDING loading from landing to target</description>
  				    <skipCondition> not ('${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list1_o}'.contains('[holding_tp.pub_move_land_2_target]') or '${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list2_o}'.contains('[holding_tp.pub_move_land_2_target]'))</skipCondition>
					<execDefinition>
						<dbProcedure name="CTRMSO.ETL_INVOKER_SP">
							<in>
								<param name="P_TYPE_NAME_I" type="STRING">holding_tp</param>
								<param name="P_METHOD_NAME_I" type="STRING">pub_move_land_2_target</param>
								<param name="P_RUN_ID_I" type="NUMBER">${RUN-ID}</param>
								<param name="P_STAGE_ID_I" type="STRING">TARGET</param>
								<param name="P_TASK_ID_I" type="STRING">IN-HOLDING-L2T</param>
								<param name="P_LOG_ID_I" type="STRING">1</param>
								<param name="P_BUSINESS_DATE_I" type="STRING">${BUSINESS-DATE}</param>
								<param name="P_SOURCE_SYSTEM_CD_I" type="STRING">${SOURCE-SYSTEM}</param>
							</in>
							<out>
								<param name="P_RET_VALUE_O" type="NUMBER"></param>
								<param name="P_RET_MESSAGE_O" type="STRING"></param>
							</out>
						</dbProcedure>
					</execDefinition>
				</stage>

				<!-- Load NON-MMI cash holding -->
				<!-- Disabling for R11 -->				
