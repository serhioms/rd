							<param name="P_LOG_ID_I" type="STRING">1</param>
							<param name="P_BUSINESS_DATE_I" type="STRING">${BUSINESS-DATE}</param>
							<param name="P_SOURCE_SYSTEM_CD_I" type="STRING">${SOURCE-SYSTEM}</param>
