								<param name="P_RET_MESSAGE_O" type="STRING"></param>
							</out>
						</dbProcedure>
					</execDefinition>
				</stage>
				<stage name="LAND-TO-TARGET-PORTFOLIO" id="HL-PORTFOLIO-L2T">
					<description>PORTFOLIO loading from landing to target</description>
  				    <skipCondition> not ('${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list1_o}'.contains('[portfolio_tp.pub_move_land_2_target]') or '${GET-SOURCE-SYSTEM-PROC-CONFIG.p_proc_list2_o}'.contains('[portfolio_tp.pub_move_land_2_target]'))</skipCondition>
					
