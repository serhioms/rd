<workflow xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="workflowDefinition.xsd">
	<name>CM-CSM-ENRICHMENT-WF</name> 
  	<externalWorkflowCode>CM-CSM-ENRICHMENT-WF</externalWorkflowCode> 
  	<finishOrchestrationASAPOnError>false</finishOrchestrationASAPOnError> 
	<description>CM-CSM-ENRICHMENT-WF ($LastChangedRevision: 3690 $)</description>

	<properties>
		<inputProperties>
			<property name="SOURCE-SYSTEM" />
			<property name="SOURCE-SYSTEM-LC" />
			<property name="RUN-ID" /> <!-- INITIALIZE-RUN.P_RUN_ID_O -->
			<property name="BUSINESS-DATE" />
			<property name="WF-EOD-EOM-INDICATOR" />
			<!-- CSM -->
			<property name="BUSINESS-DATE-FORMATTED"/><!-- INITIALIZE-RUN.BUSINESS-DATE-FORMATTED -->
			<property name="IS-MONTH-END"/>
			<!-- CSM -->
		</inputProperties>
	</properties>
	
	<!-- DELIVERY PHASE -->

	<sequential name="CM-CSM-ENRICHMENT" id="CM-CSM-ENRICHMENT">
	<!-- BEGIN : PREPARE FOR EXTRACT -->
		<externalWorkflowReference name="CM-EXPORT-LINK-INSTRUMENT-WF" id="CM-EXPORT-CSM-PRE-LINK-WF">
			<externalWorkflowCode>CM-EXPORT-LINK-INSTRUMENT-WF</externalWorkflowCode>
			<inputProperties>
				<property name="WF-EOD-EOM-INDICATOR">${WF-EOD-EOM-INDICATOR}</property>
					<property name="SOURCE-SYSTEM">${SOURCE-SYSTEM}</property>
					<property name="RUN-ID" >${RUN-ID}</property>
					<property name="BUSINESS-DATE">${BUSINESS-DATE}</property>
					<property name="BUSINESS-DATE-FORMATTED">${BUSINESS-DATE-FORMATTED}</property>
					<property name="IS-MONTH-END">${IS-MONTH-END}</property>
			</inputProperties>
		</externalWorkflowReference>
		<!-- END : PREPARE FOR EXTRACT -->
	<!-- BEGIN : CSM  -->
			<externalWorkflowReference name="CM-CSM-WF" id="CM-CSM-WF">
				<externalWorkflowCode>CM-CSM-WF</externalWorkflowCode>
				<inputProperties>
					<property name="WF-EOD-EOM-INDICATOR">${WF-EOD-EOM-INDICATOR}</property>
					<property name="SOURCE-SYSTEM">${SOURCE-SYSTEM}</property>
					<property name="RUN-ID" >${RUN-ID}</property>
					<property name="BUSINESS-DATE">${BUSINESS-DATE}</property>
					<property name="BUSINESS-DATE-FORMATTED">${BUSINESS-DATE-FORMATTED}</property>
					<property name="IS-MONTH-END">${IS-MONTH-END}</property>
				</inputProperties>
			</externalWorkflowReference>
		<!-- END : CSM  -->
		<!-- BEGIN : PREPARE FOR EXTRACT -->
		<externalWorkflowReference name="CM-EXPORT-CSM-CUT-OFF-WF" id="CM-EXPORT-CSM-CUT-OFF-WF">
			<externalWorkflowCode>CM-EXPORT-CSM-CUT-OFF-WF</externalWorkflowCode>
			<inputProperties>
				<property name="WF-EOD-EOM-INDICATOR">${WF-EOD-EOM-INDICATOR}</property>
					<property name="SOURCE-SYSTEM">${SOURCE-SYSTEM}</property>
					<property name="RUN-ID" >${RUN-ID}</property>
					<property name="BUSINESS-DATE">${BUSINESS-DATE}</property>
					<property name="BUSINESS-DATE-FORMATTED">${BUSINESS-DATE-FORMATTED}</property>
					<property name="IS-MONTH-END">${IS-MONTH-END}</property>
			</inputProperties>
		</externalWorkflowReference>
		<!-- END : PREPARE FOR EXTRACT -->
	</sequential>
</workflow>
	